{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://lme.eripotter.com"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.eripotter.com/api/:path*"
    }
  ]
} 